<main class="container">
    <%= link_to sanitize("&larr; Back to homepage"), root_path %>
    <div class="header">
      <h1>Parents</h1>
      <%= link_to "New parent",
                  new_parent_path,
                  data: { turbo_frame: dom_id(Parent.new) },
                  class: "btn btn--primary" %>
    </div>

  <%= turbo_frame_tag Parent.new %>

  <%= turbo_frame_tag "parents" do %>
    <%= render @parents %>
  <% end %>
</main>
